{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "paths": {
      "@core/*": ["./src/core/*"],
      "@auth/*": ["./src/auth/*"],
      "@fishing_operation/*": ["./src/fishing_operation/*"],
      "@operations/*": ["./src/operation/*"],
      "@sales/*": ["./src/sales/*"],
      "@transportation/*": ["./src/transportation/*"],
      "@notification/*": ["./src/notifications/*"]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
